{
  "name": "meteor-boilerplate",
  "author": "Fabien Maronnaud",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings-development.json",
    "test": "cross-env BABEL_ENV=COVERAGE TEST_BROWSER_DRIVER=puppeteer COVERAGE=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=$(pwd)/ meteor test --once --driver-package meteortesting:mocha --raw-logs && cat .coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true && cat .coverage/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js || true",
    "test-local": "cross-env TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package dispatch:mocha",
    "test-live": "meteor test --driver-package=cultofcoders:mocha",
    "lint": "npm run-script lint:javascript && npm run-script lint:markdown && npm run-script lint:style",
    "lint:javascript": "eslint . --ext .jsx --ext .js",
    "lint:javascript:fix": "eslint . --fix --ext .jsx --ext .js",
    "lint:markdown": "markdownlint \"*.md\"",
    "lint:style": "stylelint \"styles/**/*.scss\"",
    "pretest": "npm run-script lint",
    "coverage:unit": "cross-env BABEL_ENV=COVERAGE TEST_BROWSER_DRIVER=puppeteer COVERAGE=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=%cd%\\ meteor test --once --driver-package meteortesting:mocha --raw-logs",
    "coverage:watch": "cross-env BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_VERBOSE=1 COVERAGE_APP_FOLDER=%cd%\\ TEST_WATCH=1 meteor test --driver-package cultofcoders:mocha --raw-logs",
    "coverage:coverall": "cross-env cat \".coverage/lcov.info\" | \"./node_modules/coveralls/bin/coveralls.js\" || true",
    "coverage:unit:full": "npm run-script coverage:unit && npm run-script coverage:coverall",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "apollo-link-ws": "^1.0.18",
    "apollo-server-express": "^2.6.3",
    "bluebird": "^3.5.5",
    "favicons": "^5.3.0",
    "graphql": "^14.3.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.4",
    "json-buffer": "^3.0.1",
    "meteor-node-stubs": "^0.4.1",
    "mysql2": "^1.6.5",
    "path": "^0.12.7",
    "platform": "^1.3.5",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.11.0",
    "react": "^16.6.3",
    "react-apollo": "^2.5.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.6.3",
    "react-mounter": "^1.2.0",
    "react-router": "^3.2.1",
    "react-transition-group": "^2.9.0",
    "redis": "^2.8.0",
    "sequelize": "^5.8.10",
    "sequelize-redis": "^1.0.12",
    "simpl-schema": "^1.5.3",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "codecov.io": "^0.1.6",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-react": "^7.5.1",
    "faker": "^4.1.0",
    "markdownlint-cli": "^0.13.0",
    "nightmare": "^3.0.1",
    "pre-commit": "^1.2.2",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-scss": "^3.4.1"
  },
  "babel": {
    "env": {
      "COVERAGE": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "snyk": true
}
